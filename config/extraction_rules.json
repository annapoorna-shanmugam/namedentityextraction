  "finance_entities": {
    "DATE": {
      "patterns": [
        "\\b(?:January|February|March|April|May|June|July|August|September|October|November|December) \\d{1,2}, \\d{4}\\b",
        "\\b\\d{1,2}/\\d{1,2}/\\d{2,4}\\b",
        "\\b\\d{4}-\\d{2}-\\d{2}\\b"
      ],
      "context_words": [
        "on",
        "as of",
        "dated"
      ]
    },
    "ORGANIZATION": {
      "patterns": [
        "\\b[A-Z][A-Za-z0-9&.]+(?:\\s+[A-Z][A-Za-z0-9&.]+)*\\s+(?:Corp|Inc|Ltd|LLC|Corporation|Company|Enterprises?)\\b",
        "\\b[A-Z]{2,}(?:\\s+[A-Z]{2,})*\\s+(?:Corp|Inc|Ltd|LLC|Corporation|Company|Enterprises?)\\b"
      ],
      "context_words": [
        "acquired",
        "merged",
        "partnered",
        "invested",
        "reported",
        "announced"
      ]
    },
    "FINANCIAL_METRIC": {
      "patterns": [
        "(?i)\\b(?:quarterly\\s+)?revenue\\s+of\\s+\\$\\s?\\d[\\d,]*(?:\\.\\d+)?\\s?(?:million|billion|thousand|m|bn|k)?\\b",
        "(?i)\\bprofit\\s+of\\s+\\$\\s?\\d[\\d,]*(?:\\.\\d+)?\\s?(?:million|billion|thousand|m|bn|k)?\\b",
        "(?i)\\bearnings\\s+of\\s+\\$\\s?\\d[\\d,]*(?:\\.\\d+)?\\s?(?:million|billion|thousand|m|bn|k)?\\b",
        "(?i)\\bnet\\s+income\\s+of\\s+\\$\\s?\\d[\\d,]*(?:\\.\\d+)?\\s?(?:million|billion|thousand|m|bn|k)?\\b"
      ],
      "context_words": [
        "reported",
        "announced",
        "posted"
      ]
    },
    "PERCENTAGE_CHANGE": {
      "patterns": [
        "(?i)\\b(?:\\d+(?:\\.\\d+)?)%\\s*(?:increase|decrease|rise|drop|growth|decline|gain|loss)\\b|\\b(?:up|down|rose by|fell by|increased by|decreased by)\\s*\\d+(?:\\.\\d+)?%\\b"
      ],
      "context_words": [
        "from previous",
        "compared to",
        "following"
      ]
    },
