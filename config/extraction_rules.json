{
  "healthcare_entities": {
    "PATIENT": {
      "patterns": [
        "patient",
        "Mr\\.",
        "Mrs\\.",
        "Ms\\.",
        "Dr\\.",
        "individual",
        "subject"
      ],
      "context_words": ["diagnosed", "treated", "admitted", "discharged"]
    },
    "MEDICATION": {
      "patterns": [
        "aspirin",
        "ibuprofen",
        "acetaminophen",
        "metformin",
        "lisinopril",
        "amlodipine",
        "simvastatin",
        "omeprazole",
        "levothyroxine",
        "albuterol",
        "insulin",
        "warfarin",
        "prednisone",
        "hydrochlorothiazide",
        "losartan",
        "mg",
        "tablet",
        "capsule",
        "injection",
        "dose",
        "dosage",
        "\\b\\d+mg\\b",
        "\\b\\d+ml\\b",
        "\\b\\d+ units\\b",
        "\\b\\d+ tablets?\\b",
        "\\b\\d+ capsules?\\b"
      ],
      "context_words": ["prescribed", "administered", "taken", "given", "injected"]
    },
    "DISEASE": {
      "patterns": [
        "diabetes",
        "hypertension",
        "asthma",
        "pneumonia",
        "bronchitis",
        "influenza",
        "covid-19",
        "coronavirus",
        "heart disease",
        "stroke",
        "cancer",
        "tumor",
        "infection",
        "fever",
        "headache",
        "migraine",
        "depression",
        "anxiety",
        "arthritis",
        "osteoporosis"
      ],
      "context_words": ["diagnosed with", "suffering from", "history of", "symptoms of"]
    },
    "TREATMENT": {
      "patterns": [
        "surgery",
        "operation",
        "procedure",
        "therapy",
        "treatment",
        "chemotherapy",
        "radiation",
        "dialysis",
        "transplant",
        "biopsy",
        "endoscopy",
        "mri",
        "ct scan",
        "x-ray",
        "ultrasound",
        "blood test"
      ],
      "context_words": ["underwent", "received", "scheduled for", "completed"]
    },
    "SYMPTOM": {
      "patterns": [
        "pain",
        "swelling",
        "rash",
        "nausea",
        "vomiting",
        "diarrhea",
        "constipation",
        "fatigue",
        "weakness",
        "dizziness",
        "shortness of breath",
        "chest pain",
        "abdominal pain",
        "back pain",
        "joint pain"
      ],
      "context_words": ["experiencing", "complaining of", "reports", "presents with"]
    },
    "BODY_PART": {
      "patterns": [
        "heart",
        "lung",
        "kidney",
        "liver",
        "brain",
        "stomach",
        "intestine",
        "spine",
        "knee",
        "shoulder",
        "hip",
        "ankle",
        "wrist",
        "chest",
        "abdomen",
        "head",
        "neck"
      ],
      "context_words": ["in the", "of the", "located in"]
    }
  },
  "finance_entities": {
    "DATE": {
      "patterns": [
        "\\b(?:January|February|March|April|May|June|July|August|September|October|November|December) \\d{1,2}, \\d{4}\\b",
        "\\b\\d{1,2}/\\d{1,2}/\\d{2,4}\\b",
        "\\b\\d{4}-\\d{2}-\\d{2}\\b"
      ],
      "context_words": ["on", "as of", "dated"]
    },
    "ORGANIZATION": {
      "patterns": [
        "\\b[A-Z][A-Za-z0-9&. ]+ (Corp|Inc|Ltd|LLC|Corporation|Company|Enterprises?)\\b",
        "company", "corporation", "inc", "ltd", "llc", "enterprise", "firm", "organization"
      ],
      "context_words": ["acquired", "merged", "partnered", "invested", "reported", "announced"]
    },
    "FINANCIAL_METRIC": {
      "patterns": [
        "revenue of \\$\\s?\\d+[\\d,.]*\\s?(million|billion|thousand|m|bn|k)?",
        "profit of \\$\\s?\\d+[\\d,.]*\\s?(million|billion|thousand|m|bn|k)?",
        "earnings of \\$\\s?\\d+[\\d,.]*\\s?(million|billion|thousand|m|bn|k)?",
        "quarterly revenue of \\$\\s?\\d+[\\d,.]*\\s?(million|billion|thousand|m|bn|k)?",
        "net income of \\$\\s?\\d+[\\d,.]*\\s?(million|billion|thousand|m|bn|k)?"
      ],
      "context_words": ["reported", "announced", "posted"]
    },
    "PERCENTAGE_CHANGE": {
      "patterns": [
        "\\b\\d+\\.?\\d*% (increase|decrease|rise|drop|growth|decline|gain|loss)\\b",
        "\\b(up|down|rose by|fell by|increased by|decreased by) \\d+\\.?\\d*%\\b"
      ],
      "context_words": ["from previous", "compared to", "following"]
    },
    "STOCK_PRICE_CHANGE": {
      "patterns": [
        "stock price (rose|increased|jumped|soared|fell|dropped|declined|plunged|slipped|advanced|retreated) by \\d+\\.?\\d*%",
        "\\b\\d+\\.?\\d*% (rise|gain|loss|increase|decrease)\\b"
      ],
      "context_words": ["following", "after", "announcement"]
    },
    "PERSON": {
      "patterns": [
        "CEO [A-Z][a-z]+ [A-Z][a-z]+",
        "\\b([A-Z][a-z]+\\s[A-Z][a-z]+)\\b"
      ],
      "context_words": ["CEO", "CFO", "President", "Manager", "Director", "attributed"]
    },
    "SECTOR": {
      "patterns": [
        "technology sector",
        "finance sector",
        "healthcare sector",
        "energy sector",
        "industrial sector",
        "consumer sector",
        "digital payment",
        "digital payment service",
        "digital payment services",
        "\\b[A-Za-z]+ sector\\b"
      ],
      "context_words": ["in", "within", "from"]
    },

    "CURRENCY": {
      "patterns": [
        "usd", "eur", "gbp", "inr", "yen", "dollar", "euro", "rupee", "pound", "currency",
        "\\$\\s?\\d+[\\d,.]*\\s?(million|billion|thousand|m|bn|k)?",
        "€\\s?\\d+[\\d,.]*\\s?(million|billion|thousand|m|bn|k)?",
        "£\\s?\\d+[\\d,.]*\\s?(million|billion|thousand|m|bn|k)?",
        "¥\\s?\\d+[\\d,.]*\\s?(million|billion|thousand|m|bn|k)?",
        "₹\\s?\\d+[\\d,.]*\\s?(million|billion|thousand|m|bn|k)?"
      ],
      "context_words": ["converted", "exchanged", "valued at"]
    },
    "TRANSACTION": {
      "patterns": ["transaction", "payment", "transfer", "deposit", "withdrawal", "purchase", "sale", "deal"],
      "context_words": ["completed", "initiated", "pending", "failed"]
    },
    "ACCOUNT": {
      "patterns": ["account", "bank account", "savings", "checking", "current account", "number"],
      "context_words": ["opened", "closed", "credited", "debited"]
    }
  },
  "finance_events": {
    "MERGER": {
      "triggers": ["merged", "acquired", "consolidated"],
      "attributes": ["company1", "company2", "date", "value"]
    },
    "TRANSACTION": {
      "triggers": ["transaction", "payment", "transfer"],
      "attributes": ["amount", "currency", "date", "account"]
    },
    "IPO": {
      "triggers": ["ipo", "public offering", "listed"],
      "attributes": ["company", "date", "value"]
    },
    "EARNINGS_REPORT": {
      "triggers": [
        "reported", "announced", "earnings", "quarterly results", "financial results",
        "growth", "profit", "net income", "results", "quarterly revenue", "quarterly profit",
        "revenue increase", "revenue growth", "reported revenue", "announced revenue"
      ],
      "attributes": ["company", "date", "amount", "growth", "period"]
    },
    "STOCK_MOVEMENT": {
      "triggers": [
        "increased", "decreased", "fell", "dropped", "stock price", "gained", "lost", "jumped", "plunged", "soared", "declined",
        "rise", "gain", "loss", "up", "down", "surged", "slipped", "advanced", "retreated",
        "price rose", "stock rose", "share price rose", "rose by", "rose to"
      ],
      "attributes": ["company", "date", "percent", "reason"]
    }
  },
  "healthcare_events": {
    "ADMISSION": {
      "triggers": ["admitted", "hospitalized", "checked in"],
      "attributes": ["date", "hospital", "reason", "patient"]
    },
    "DISCHARGE": {
      "triggers": ["discharged", "released", "sent home"],
      "attributes": ["date", "condition", "instructions", "patient"]
    },
    "DIAGNOSIS": {
      "triggers": ["diagnosed", "found to have", "identified with"],
      "attributes": ["disease", "patient", "date", "physician"]
    },
    "PRESCRIPTION": {
      "triggers": ["prescribed", "given medication", "started on"],
      "attributes": ["medication", "dosage", "frequency", "patient"]
    },
    "SURGERY": {
      "triggers": ["surgery", "operation", "surgical procedure"],
      "attributes": ["procedure", "patient", "date", "surgeon", "outcome"]
    },
    "TEST_RESULT": {
      "triggers": ["test showed", "results indicate", "lab values"],
      "attributes": ["test_type", "result", "normal_range", "patient"]
    }
  }
}